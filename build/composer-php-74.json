{
    "name": "driftingly/rector-laravel",
    "type": "rector-extension",
    "license": "MIT",
    "description": "Rector upgrades rules for Laravel Framework",
    "require": {
        "php": "^7.4 || ^8.0",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "RectorLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["stubs"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
